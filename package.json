{
  "name": "chatbot-chrome-extension",
  "version": "1.0.0",
  "description": "This is a chrome extension to inject a dify chatbot on any pages",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./dist",
    "watch": "webpack --watch --config webpack.dev.cjs",
    "build:dev": "webpack --config webpack.dev.cjs && npx tailwindcss -o dist/styles.css",
    "build:prod": "webpack --config webpack.prod.cjs && npx tailwindcss -o dist/styles.css"
  },
  "keywords": [
    "dify chatbot"
  ],
  "author": "",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "postcss": "^8.4.41",
    "postcss-loader": "^8.1.1",
    "tailwindcss": "^3.4.10",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1"
  }
}
